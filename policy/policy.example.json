{
  "default_allow": false,
  "shell_start": {
    "alice": ["priv-lvl=15"],
    "bob": ["priv-lvl=1"]
  },
  "rules": [
    {
      "id": "allow-show",
      "priority": 10,
      "effect": "allow",
      "pattern": "show(\\s+.*)?"
    },
    {
      "id": "deny-config",
      "priority": 50,
      "effect": "deny",
      "pattern": "(configure|conf)(\\s+.*)?"
    },
    {
      "id": "allow-config-alice",
      "priority": 60,
      "effect": "allow",
      "users": ["alice"],
      "pattern": "(configure|conf)(\\s+.*)?"
    }
  ]
}
