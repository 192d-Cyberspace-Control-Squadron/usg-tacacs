{
  "default_allow": false,
  "shell_start": {
    "alice": ["priv-lvl=15"],
    "bob": ["priv-lvl=1"]
  },
  "ascii_prompts": {
    "username": "Login:",
    "password": "Secret:"
  },
  "ascii_user_prompts": {
    "alice": "Alice login:",
    "bob": "Bob login:"
  },
  "ascii_password_prompts": {
    "alice": "Alice secret:",
    "bob": "Bob secret:"
  },
  "ascii_port_prompts": {
    "ttyS0": "Console login:",
    "vty0": "VTY login:"
  },
  "ascii_remaddr_prompts": {
    "10.0.0.1": "Remote 10.0.0.1 login:"
  },
  "ascii_messages": {
    "success": "authentication succeeded",
    "failure": "invalid credentials",
    "abort": "authentication aborted"
  },
  "rules": [
    {
      "id": "allow-show",
      "priority": 10,
      "effect": "allow",
      "pattern": "show(\\s+.*)?"
    },
    {
      "id": "deny-config",
      "priority": 50,
      "effect": "deny",
      "pattern": "(configure|conf)(\\s+.*)?"
    },
    {
      "id": "allow-config-alice",
      "priority": 60,
      "effect": "allow",
      "users": ["alice"],
      "pattern": "(configure|conf)(\\s+.*)?"
    }
  ]
}
